<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 雲端備份規則 -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- 裝置轉移規則 -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>
